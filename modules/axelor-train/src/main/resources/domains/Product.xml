<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models https://axelor.com/xml/ns/domain-models/domain-models_7.1.xsd">

    <module name="product" package="com.axelor.product.db"/>

    <entity name="Products"  cacheable="true" repository="default">
        <string name="productCode" required="true" title="Product Code"/>
        <string name="productName" required="true" title="Product Name"/>
        <integer name="productNetWeight" required="true" title="Product Net Weight"/>
        <integer name="productGrossWeight" required="true" title="Product Gross Weight"/>
        <string name="packingType" required="true" title="Packing Type"/>
    </entity>
</domain-models>
