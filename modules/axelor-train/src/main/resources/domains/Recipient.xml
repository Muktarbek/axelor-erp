<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models https://axelor.com/xml/ns/domain-models/domain-models_7.1.xsd">

    <module name="recipient" package="com.axelor.recipient.db"/>

    <entity name="Recipient"  cacheable="true" repository="default">
      <string name="type" title="type"/>
      <string name="companyName" title="companyName"/>
      <string name="taxpayerID"  title="taxpayerID"/>
      <string name="okpo"  title="okpo"/>
      <string name="country"  title="country"/>
        <string name="personalINN"  title=""/>
      <string name="firstName"  title="firstName"/>
      <string name="lastName"  title="lastName"/>
      <string name="patronymic" title="patronymic"/>
        <one-to-many name="products" ref="com.axelor.product.db.Products"  title="Products"/>
    </entity>
</domain-models>
